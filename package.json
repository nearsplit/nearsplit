{
  "name": "near-split",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "eslint --fix \"**/*.{ts,tsx,md}\"",
    "deploy:contract": "cd apps/contract && pnpm run deploy",
    "build:contract": "cd apps/contract && pnpm build"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.11.0"
}